{
    "name": "fsi/datasource",
    "type": "library",
    "description": "FSi DataSource Component",
    "keywords": ["component", "datasource", "fsi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Szczepan Cieslik",
            "email": "szczepan@fsi.pl"
        },
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/event-dispatcher": "^2.2|^3.0|^4.0",
        "symfony/options-resolver": "^2.6|^3.0|^4.0",
        "fsi/data-indexer" : "0.9.*"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "doctrine/common": "^2.4",
        "doctrine/collections": "^1.1",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Component\\DataSource\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\Component\\DataSource\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
